<?xml version="1.0" encoding="utf-8"?>
<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		  Priority="30"
		  RightName="Page:TowarModelPage"
		  xmlns="http://www.enova.pl/schema/form.xsd"
		  xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/form.xsd"
		  DataType="Soneta.Towary.Towar, Soneta.Handel">
	
	<Page Name="TowarModelPage" CaptionHtml="Modele" DataContext="{DataSource}">
		<Group CaptionHtml="Ogólne informacje">
			<Label CaptionHtml="Opis modelu towaru" Width="50" />
			<Row>
				<Field CaptionHtml="Model" Width="25" LabelWidth="20" EditValue="{Features.Model}" IsReadOnly="true" />
				<Field CaptionHtml="Marka" Width="25" LabelWidth="20" EditValue="{Features.Marka}" Visibility="false"/>
			</Row>
			<Row>
				<Field CaptionHtml="Data ostatniego przegladu" Width="25" LabelWidth="20" EditValue="{Features.DataOstatniegoPrzegladu}" />
				<Field CaptionHtml="Numer seryjny" Width="30" LabelWidth="20" EditValue="{Features.NumerSeryjny}" />
			</Row>
		</Group>
		<Group CaptionHtml="Dane techniczne">
			<Field CaptionHtml="Rok produkcji" Width="15" LabelWidth="20" EditValue="{Features.RokProdukcji}" />
		</Group>
	</Page>
</DataForm>
